{
    "name": "@Broadshield/github-action-download-release-asset",
    "version": "1.0.1",
    "description": "Download a release asset from a release",
    "main": "dist/main.js",
    "repository": {
        "type": "git",
        "url:": "git@github.com:Broadshield/github-action-download-release-asset.git"
    },
    "keywords": [
        "actions",
        "node",
        "maven",
        "gradle",
        "tagging"
    ],
    "private": true,
    "author": "Jamie Nelson <jamie@wearsafe.com>",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.6.0",
        "@actions/github": "^5.0.0",
        "@octokit/openapi-types": "^11.2.0",
        "@octokit/request": "^5.6.3",
        "@octokit/request-error": "^2.1.0",
        "@octokit/rest": "^18.12.0",
        "ansi-colors": "^4.1.1"
    },
    "devDependencies": {
        "@commitlint/cli": "^16.2.1",
        "@commitlint/config-conventional": "^16.2.1",
        "@tsconfig/node16": "^1.0.2",
        "@types/jest": "^27.4.1",
        "@types/js-yaml": "^4.0.5",
        "@types/lodash": "^4.14.179",
        "@types/node": "^17.0.21",
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "@typescript-eslint/typescript-estree": "^5.14.0",
        "@vercel/ncc": "^0.33.3",
        "cz-conventional-changelog": "^3.3.0",
        "dotenv": "^16.0.0",
        "eslint": "^8.10.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-import-resolver-node": "^0.3.6",
        "eslint-plugin-github": "^4.3.5",
        "eslint-plugin-import": "^2.25.4",
        "eslint-plugin-jest": "^26.1.1",
        "eslint-plugin-prettier": "^4.0.0",
        "eslint-plugin-security": "^1.4.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "husky": "^7.0.4",
        "jest": "^27.5.1",
        "jest-circus": "^27.5.1",
        "jest-junit": "^13.0.0",
        "js-yaml": "^4.1.0",
        "lint-staged": "^12.4.3",
        "markdownlint-cli": "^0.31.1",
        "pinst": "^3.0.0",
        "prettier": "^2.5.1",
        "rimraf": "^3.0.2",
        "semantic-release": "^19.0.2",
        "stylelint": "^14.5.3",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-rational-order": "^0.1.2",
        "stylelint-config-standard": "^25.0.0",
        "stylelint-formatter-github": "^1.0.1",
        "stylelint-order": "^5.0.0",
        "stylelint-prettier": "^2.0.0",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.7.0",
        "tslib": "^2.3.1",
        "typescript": "^4.6.2",
        "typescript-eslint-language-service": "^5.0.0",
        "yarn-or-npm": "^3.0.1"
    },
    "scripts": {
        "test": "NODE_ENV=testing jest",
        "test-ci": "NODE_ENV=testing jest --ci --reporters=default --reporters=jest-junit",
        "build": "rimraf lib && tsc --project ./tsconfig.json",
        "package": "yon run build;ncc build lib/main.js -o dist --source-map --license licenses.txt --target es2021",
        "format": "prettier --write \"./**/*.{ts,js,json,md}\"",
        "check:format": "prettier --list-different \"./**/*.{ts,js,json,md}\"",
        "lint:fix": "eslint . --fix --ext .js,.ts",
        "lint:markdown:fix": "yarn lint:markdown --fix",
        "lint:markdown": "markdownlint \"**/*.md\" --config=.markdownlint.json --ignore-path=.markdownlintignore",
        "lint": "eslint . --ext .js,.ts",
        "postinstall": "husky install",
        "pre-commit": "lint-staged && yon run build && git add dist",
        "pre-push": "yarn check:format",
        "preversion": "echo 'Running test';yon test",
        "prepare": "husky install"
    },
    "jest-junit": {
        "suiteNameTemplate": "{filepath}",
        "classNameTemplate": "{classname}",
        "titleTemplate": "{title}"
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run release; git add dist"
        }
    }
}
